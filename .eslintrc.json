{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "camelcase": 0,
        "no-undef": 0,
        "max-len": 0,
        "consistent-return": 0,
        "no-alert": 0,
        "radix": 0,
        "object-shorthand": 0,
        "no-unused-vars": 0,
        "object-curly-newline": 0,
        "no-return-assign": 0,
        "no-useless-escape": 0
    }
}